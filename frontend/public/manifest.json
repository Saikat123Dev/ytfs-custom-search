{
  "manifest_version": 3,
  "name": "YT Sniper",
  "version": "1.0",
  "description": "ðŸŽ¯ Instantly search and jump to precise YouTube timestamps with intelligent snippet previews.",
  "minimum_chrome_version": "116",
  "background": {
    "service_worker": "js/service-worker.js"
  },
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
    
  },
  "action": {
    "default_title": "Click to open panel",
    "default_icon": {
      "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
    }
  },
  "side_panel": {
    "default_path": "index.html"
  },
  "permissions": [
    "tabs",
    "activeTab",
    "sidePanel"
  ],
  "host_permissions": [
    "https://ytfs-custom-search-l7s5.onrender.com/search"
  ]
}